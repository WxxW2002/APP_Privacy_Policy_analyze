    <script>
      (function() {
        // 错误上报
        function ready(cb) {
          if (window.NEJsbridge) {
            cb();
          } else {
            document.addEventListener('NEJsbridgeReady', cb, false);
          }
        }
        function invoke() {
          for (var _len = arguments.length, args = Array(_len), _key = 0; _key < _len; _key++) {
            args[_key] = arguments[_key];
          }
          if (window.WebViewJavascriptBridge) {
            var _window$WebViewJavasc;
            return (_window$WebViewJavasc = window.WebViewJavascriptBridge).callHandler.apply(_window$WebViewJavasc, args);
          } else {
            ready(function() {
              var _window$NEJsbridge;
              (_window$NEJsbridge = window.NEJsbridge).invoke.apply(_window$NEJsbridge, args);
            });
          }
        }
        function isChosenDevice() {
          var isChosenDevice = false;
          try {
            var deviceId = navigator.userAgent.match(/device-id\/(\w+)/)[1];
            var num = Number(deviceId.slice(-1));
            if (num === 0) {
              isChosenDevice = true;
            }
          } catch (e) {}
          return isChosenDevice;
        }
        window.environment === 'online' && window.addEventListener('error', function(e) {
          if (e instanceof ErrorEvent) {
            var colno = e.colno,
              error = e.error,
              filename = e.filename,
              lineno = e.lineno,
              message = e.message;
            invoke('errorReport', {
              msg: "\u3010FERuntimeError\u3011".concat(location.href, ' ').concat(filename, ": error in line ").concat(lineno, " at column ").concat(colno, ", msg: ").concat(message).concat(error && error.stack ? ', stack is ' + error.stack : '')
            });
          } else {
            var resourceUrl = e.target.src || e.target.href;
            var type = e.target.tagName.toLowerCase();
            var errorTypeMap = {
              'script': 'ScriptLoadError',
              'link': 'StylesheetLoadError',
              'img': 'ImageLoadError',
              'audio': 'AudioLoadError',
              'video': 'VideoLoadError'
            };
            if (resourceUrl && type === 'script' && isChosenDevice()) {
              invoke('errorReport', {
                msg: "\u3010FEResourceError\u3011".concat(location.href, ' ').concat(errorTypeMap[type], ": ").concat(resourceUrl)
              });
            }
          }
        }, true);
      })();
    </script>
