        var customHeader = {
          isEnvReady: false,
          totalHeight: 0,
          contentHeight: 0,
          safeHeight: 0,
          leftIcon: [],
          rightIcon: [],
          cbQueue: [],
          shouldRenderHeader: false,
          isBlockInsert: false,
          init: function init() {
            var _this = this;
            if (shouldNotWork) {
              return;
            }
            getAppInfo(function(appInfo) {
              if (appInfo && appInfo.env && appInfo.env.fullScreen) {
                _this.shouldRenderHeader = true;
                _this.appInfo = appInfo.env;
                _this.initVariables(appInfo.env);
                invoke('disableBounce', {});
                _this.checkBody(function() {
                  _this.insertPlaceholderBlock();
                });
                _this.setLeftIcon();
                _this.customHeaderAction();
              } else {
                _this.customHeaderAction();
              }
            });
          },
          checkBody: function checkBody(cb) {
            var self = this;
            var countdown = null;
            if (requestAnimationFrame) {
              countdown = requestAnimationFrame;
            } else {
              countdown = function countdown(cb) {
                setTimeout(cb, 16);
              };
            }
            countdown(function() {
              if (document.body) {
                cb();
              } else {
                countdown(self.checkBody);
              }
            });
          },
          getEventEmitter: function getEventEmitter() {
            if (!window.EventEmitter) {
              return {
                emit: function emit() {},
                on: function on() {}
              };
            }
            !this.ee && (this.ee = new window.EventEmitter());
