          },
          addRightIcon: function addRightIcon(arr) {
            var _this2 = this;
            if (!(Object.prototype.toString.call(arr) == '[object Array]' && arr.length)) {
              return;
            }
            var iconArr = this.rightIcon;
            arr.forEach(function(item) {
              if (!item.type || !item.name) {
                return;
              }
              var existIconIndex = iconArr.findIndex(function(icon) {
                return icon.type == item.type && icon.name == item.name;
              });
              if (existIconIndex != -1) {
                iconArr[existIconIndex] = Object.assign({}, iconArr[existIconIndex], item);
                _this2.getEventEmitter().emit('setRightIcon', iconArr);
              } else {
                if (iconArr.length >= 2) {
                  iconArr = iconArr.slice(0, 2);
                  return;
                }
                if (item.type == 'custom') {
                  item.name && item.icon && item.icon.light && item.icon.dark && typeof item.callback == 'function' && iconArr.push(item);
                } else if (Object.keys(ICON_MAP).includes(item.type)) {
                  iconArr.push(Object.assign({}, ICON_MAP[item.type], item));
                }
                iconArr.sort(function(a, b) {
                  if (a.type == 'share') {
                    if (b.type == 'custom' && b.name == 'collect') {
                      return 1;
                    } else {
                      return -1;
                    }
                  } else if (a.type == 'custom' && a.name == 'collect') {
                    return -1;
                  } else {
                    return 1;
                  }
                });
                _this2.getEventEmitter().emit('setRightIcon', _this2.rightIcon);
                console.log('【customHeader】更新右边icon:', _this2.rightIcon);
              }
            });
          },
          changeStatusBar: function changeStatusBar(type) {
            setStatusBar(type || 'default');
