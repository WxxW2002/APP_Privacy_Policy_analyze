                  break;
                default:
                  headerStyle += 'position:relative;';
              }
              container.style = headerStyle;
              container.id = 'header_placeholder';
              var parent = document.body;
              console.log('document.body', parent);
              console.log('document.body.childNodes[0]', parent.childNodes[0]);
              parent.insertBefore(container, parent.childNodes[0]);
              console.log('【customHeader】成功插入自定义头占位块');
            } catch (e) {
              console.log(e);
              self.isBlockInsert = false;
              console.warn('【customHeader】自定义头占位块插入失败');
            }
          },
          setLeftIcon: function setLeftIcon() {
            this.leftIcon = [EMPTY_ICON];
