        function compareAppVersion(compare) {
          var compareVs = compare.split("."),
            version = getAppVersion(),
            currentVs = version.split("."),
            i = 0;
          if (compare == version) {
            return 0;
          }
          function checkVersion(i) {
            if (+compareVs[i] < +currentVs[i]) {
              return 1;
            }
            if (+compareVs[i] > +currentVs[i]) {
              return -1;
            }
            i++;
            return checkVersion(i);
          }
          return checkVersion(i);
        }
        function getAppVersion() {
          var version = navigator.userAgent.toLowerCase().match(/moutaiapp\/(\d+\.\d+\.\d+)/);
          if (version) {
            return version[version.length - 1];
          }
          return '0.0.0';
        }
        function getAppInfo(cb) {
          if (top.window && top.window.cyclopes) {
            setTimeout(function() {
              var res = {
                env: {
                  fullScreen: 0
                }
              };
              if ($('.YX_M_F48CCC').size()) {
                res = {
                  env: {
                    fullScreen: 1,
                    safeAreaStatusBar: 18,
                    safeAreaNavigationBar: 48
                  }
                };
              }
              cb(res);
            }, 1000);
            return;
          }
          if (/android/i.test(navigator.userAgent)) {
            invoke('appInfo', {}, function(appInfo) {
              console.log('【customHeader】AppInfo: ', appInfo);
              try {
                if (appInfo.code && appInfo.code == 404 && /appConfig=1_1_1/.test(location.href)) {
                  appInfo = {
                    env: {
                      fullScreen: 1,
                      notchScreen: 1,
                      safeAreaBottomOffset: 0,
                      safeAreaNavigationBar: 43,
                      safeAreaStatusBar: 30
                    }
                  };
                }
              } catch (e) {}
              cb(appInfo);
            });
          } else {
            let fallback = {
              "env": {
                "screenWidth": 375,
                "system": "iOS12.0",
                "version": "1.1.2",
                "fullScreen": true,
                "hideNavBarBackBtn": false,
                "windowHeight": 667,
                "notchScreen": 0,
                "screenHeight": 667,
                "windowWidth": 375,
                "safeAreaBottomOffset": 0,
                "safeAreaStatusBar": 20,
                "safeAreaNavigationBar": 44
              }
            }
            // 兼容低版本可能初始拿不到的情况                
            cb(window.WebViewJavascriptBridge.appInfo || fallback);
          }
        }
        function resetStatusAndCloseWebview() {
          if (window.NEJsbridge && typeof window.NEJsbridge.beforeExitWebView == 'function') {
            if (window.NEJsbridge.beforeExitWebView()) {
              setStatusBar('default');
              closeWebview();
            } else {}
          } else {
            setStatusBar('default');
            closeWebview();
          }
        }
        function setStatusBar(type) {
          invoke('setStatusBarStyle', {
            style: type
          });
        }
        function closeWebview() {
          invoke('exitWebView', {
            type: 0
          });
        }
        function goBack() {
          if (history.length > 1) {
            var currentUrl = location.href;
            history.go(-1);
            setTimeout(function() {
              var newUrl = location.href;
              if (newUrl == currentUrl) {
                resetStatusAndCloseWebview();
              }
            }, 300);
          } else {
            resetStatusAndCloseWebview();
          }
        }
        function share() {
          invoke('setShareToSNSCallback', {});
        }
      })();
    </script>
  </head>
