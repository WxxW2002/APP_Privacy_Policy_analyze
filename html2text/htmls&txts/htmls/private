          },
          addCustomHeaderCb: function addCustomHeaderCb(cb) {
            if (shouldNotWork) {
              typeof cb == 'function' && cb();
              return;
            }
            if (!this.isEnvReady) {
              this.cbQueue.push(cb);
            } else {
              typeof cb == 'function' && cb();
            }
          },
          customHeaderAction: function customHeaderAction() {
            this.isEnvReady = true;
            while (this.cbQueue.length) {
              var cb = this.cbQueue.shift();
              typeof cb == 'function' && cb();
            }
          },
          insertPlaceholderBlock: function insertPlaceholderBlock() {
            var self = this,
              env = this.appInfo;
            try {
              if (self.isBlockInsert) {
                return;
              }
              self.isBlockInsert = true;
              var container = document.createElement('div');
              var headerStyle = 'height:' + (env.safeAreaNavigationBar + env.safeAreaStatusBar) + 'px;z-index:99;';
