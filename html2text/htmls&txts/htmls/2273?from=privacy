    <div id="app"></div>
    <script type="text/javascript">
    var baseFontSize = 50;
    var baseWidth = 750;
    function setRem() {
        let clientWidth = document.documentElement.clientWidth || window.innerWidth;
        let size = Math.floor(clientWidth / baseWidth * baseFontSize);
        if (getQueryValue('isWeb') && getQueryValue('isWeb') === 'true') {
            return;
        }
        document.documentElement.style.fontSize = size + 'px';
    }
    setRem();
    // 改变窗口大小时重新设置 rem
    window.onresize = function () {
        setRem();
    };

    function getQueryValue(queryName) {
        let reg = new RegExp('(^|&)' + queryName + '=([^&]*)(&|$)', 'i');
        let r = window.location.search.substr(1).match(reg);
        if (r != null) {
            return decodeURI(r[2]);
        } else {
            return null;
        }
    }
    window['__perf_white_screen'] = performance.now();
    </script>
